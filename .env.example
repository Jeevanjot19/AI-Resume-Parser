# PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=resume_parser
DATABASE_URL=postgresql://postgres:password@postgres:5432/resume_parser

# Redis
REDIS_URL=redis://redis:6379/0

# ElasticSearch
ELASTICSEARCH_URL=http://elasticsearch:9200

# JWT
JWT_SECRET_KEY=your-secret-key-here
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# API
API_V1_STR=/api/v1
PROJECT_NAME=AI Resume Parser
BACKEND_CORS_ORIGINS=["http://localhost:8000", "http://localhost:3000"]

# Monitoring
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin

# Sentry
SENTRY_DSN=

# Model Paths
NER_MODEL_PATH=models/ner
CLASSIFICATION_MODEL_PATH=models/classification
SIMILARITY_MODEL_PATH=models/similarity

# OCR
TESSERACT_PATH=/usr/bin/tesseract